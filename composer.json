{
    "name": "cego/seamless-wallet-interface",
    "description": "Package for interacting with the seamless wallet service",
    "type": "project",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Niki Zakariassen",
            "email": "niza@cego.dk"
        },
        {
            "name": "Nikolaj Boel Jensen",
            "email": "nbj@creationsftw.com"
        }
    ],

    "require": {
        "php": "^7.4",
        "ramsey/uuid": "^4.1",
        "nesbot/carbon": "^2.42",
        "illuminate/http": "^7.0|^8.0",
        "illuminate/support": "^7.0|^8.0",
        "guzzlehttp/guzzle": "^6.5.5|^7.0.1"
    },

    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "phpstan/phpstan": "^0.12",
        "phpunit/phpunit": "^9.0"
    },

    "autoload": {
        "psr-4": {
            "Cego\\": "src/Cego"
        }
    },

    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    }
}
