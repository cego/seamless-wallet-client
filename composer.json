{
    "name": "cego/seamless-wallet-client",
    "description": "Package for interacting with the seamless wallet service",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Niki Ewald Zakariassen",
            "email": "niza@cego.dk"
        },
        {
            "name": "Nikolaj Boel Jensen",
            "email": "nbj@cego.dk"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "nesbot/carbon": "^2.42",
        "illuminate/http": "^7.0|^8.0",
        "illuminate/support": "^7.0|^8.0",
        "guzzlehttp/guzzle": "^6.5.5|^7.0.1",
        "vlucas/phpdotenv": "^5.2",
        "phpoption/phpoption": "^1.7",
        "nbj/property-container": "^1.1",
        "ext-json": "*",
        "cego/service-client-base": "^0.0.3"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "phpstan/phpstan": "^0.12",
        "phpunit/phpunit": "^9.0",
        "ramsey/uuid": "^4.1",
        "cego/request-insurance": "^0.1.3",
        "orchestra/testbench": "^6.13"
    },
    "suggest": {
        "cego/request-insurance": "Allows for sending asynchronous requests"
    },
    "autoload": {
        "psr-4": {
            "Cego\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    }
}
